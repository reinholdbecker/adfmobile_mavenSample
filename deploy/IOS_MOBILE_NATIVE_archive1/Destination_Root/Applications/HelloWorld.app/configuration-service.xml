<?xml version="1.0" encoding="UTF-8" ?>
<adfmf-config-service-configuration-data xmlns="http://xmlns.oracle.com/adf/mf">
    <!-- 
     This data specifies the iOS bundle files that should be managed by the 
     configuration service.  Currently we only manage the following:
            - connections.xml
     -->
